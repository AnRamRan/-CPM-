	"fpRight": [ "right_arm_c", "right_chestplate", "right_vambrace", "right_glove" ],

	"variables": {
		"skin" : "tex.dragon",
		"mouth_item" : "inv.main0",

		"true_speed_y" : "clamp(speed_y + 0.1, -0.3, 0.3)",
		"true_sprint" : "is_sprinting && !is_sneaking",
		"true_swim" : "is_in_water && var.true_sprint",

		"swing_arc" : "if(swing_progress > 0, -sin(swing_progress * pi) * 1.0, 0)",
		"swing_r" : "cos(limb_swing * 2 / pi) * limb_speed * -0.33",
		"swing_l" : "cos(pi + limb_swing * 2 / pi) * limb_speed * -0.33",
		"swing_r_leg" : "cos(limb_swing * 2 / pi) * limb_speed * 0.33",
		"swing_l_leg" : "cos(pi + limb_swing * 2 / pi) * limb_speed * 0.33",
		"swing_bob" : "cos(limb_swing * 4 / pi) * limb_speed",

		"neck_yaw" : "-clamp(head_yaw, -45, 45) * 0.4",
		"neck_pitch" : "clamp(head_pitch, -60, if(is_sneaking, 0, 30)) * if(var.true_swim, 0, 0.4) + if(is_riding, 18, 0)",

		"has_elytra" : "inv.chestplate == item.elytra",
		"is_flying" : "var.true_sprint && !is_on_ground && !is_in_water && var.has_elytra",
		"breath_fire" : "swing_progress > 0 && (inv.mainhand == item.fire_charge || inv.mainhand == item.flint_and_steel)",
		"open_mouth" : "var.true_sprint || var.breath_fire || var.true_speed_y > 0.1 || var.mouth_item != item.air",
		"breath_offset" : "if(var.true_sprint, cos(age / 6) * 0.225, cos(age / 18) * 0.125)",
		"loft_tail" : "var.true_sprint || is_riding || is_sneaking",
		"be_upright" : "current_pose == pose.swimming || current_pose == pose.fall_flying || current_pose == pose.sleeping",
		"should_fall" : "!is_on_ground && !is_in_water && var.true_sprint",

		"base_body_pitch" : "90 + todeg(-body.ry)",
		"rad_yaw" : "torad(yaw)",
		"speed_forward" : "speed_x*sin(var.rad_yaw)-speed_z*cos(var.rad_yaw)",
		"speed_side" : "-speed_x*cos(var.rad_yaw)-speed_z*sin(var.rad_yaw)",

		"show_helmet" : true,
		"show_armour" : true,
		"show_held_items" : true,
		"saddled" : "inv.main34 == item.saddle"
	},
	"tickVars": {
		"fall_time" : ["float", 180, "if(!is_on_ground && !is_in_water, tvl.fall_time - 24, 180)"]
	},

